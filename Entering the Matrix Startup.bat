for /f %%i in ('powershell -command "[int](Get-Date).AddSeconds(5).ToUniversalTime().Subtract((Get-Date \"1/1/1970\")).TotalSeconds"') do set "end=%%i"

:matrixLoop
set "line="
set /a counter=0
:inner
set /a counter+=1
set "line=!line!!random!!random!"
if !counter! lss 60 goto inner
<nul set /p=!line!
echo.

for /f %%t in ('powershell -command "[int](Get-Date).ToUniversalTime().Subtract((Get-Date \"1/1/1970\")).TotalSeconds"') do set "now=%%t"
if !now! lss !end! goto :matrixLoop

exit
